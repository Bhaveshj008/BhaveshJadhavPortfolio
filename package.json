{
  "name": "vite-1st-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",  
    "dev:backend": "cd backend && node server.js", 
    "build:frontend": "cd frontend && npm install --force",
    "build:backend": "cd backend && npm install --force",
    "build": "npm run build:frontend && npm run build:backend",
    "start": "npm run dev"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
