{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/backend/server.js" },
      { "src": "/blog/(.*)", "dest": "/frontend/dist/index.html" },
      { "src": "/project/(.*)", "dest": "/frontend/dist/index.html" },
      { "src": "/allblogs", "dest": "/frontend/dist/index.html" },
      { "src": "/(.*)", "dest": "/frontend/dist/index.html" }
        
        
    ]
  }
  
